#!/Users/passive/Desktop/kit-workbench/docs/toolkit/python/venv/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'Pubtest-Utility','console_scripts','tool'
__requires__ = 'Pubtest-Utility'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Pubtest-Utility', 'console_scripts', 'tool')()
    )
