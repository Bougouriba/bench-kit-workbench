{
  "name": "kisia-workbench",
  "version": "0.2.0",
  "description": "",
  "main": "./build/out/main/main.js",
  "scripts": {
    "start": "electron .",
    "start:windows": "electron . --windows-ui",
    "start:linux": "electron . --linux-ui",
    "start:mac": "electron . --mac-ui",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -fr build dist",
    "watch:pack": "webpack --progress --colors --watch --config webpack.browser.js",
    "watch:tsc": "tsc -w",
    "watch:assets": "node ./scripts/extract.js && node ./scripts/copyassets.js watch",
    "watch": "concurrently \"yarn watch:tsc\" \"yarn watch:assets\" \"yarn watch:pack\" ",
    "build:pack": "webpack --config webpack.browser.js",
    "build": "tsc && node ./scripts/extract.js && node ./scripts/copyassets.js && yarn build:pack",
    "build:all": "yarn build && yarn start",
    "extract": "node scripts/extract.js",
    "pack": "yarn build && electron-builder",
    "dist": "yarn build && electron-builder",
    "dist:linux": "yarn build && electron-builder --linux",
    "dist:mac": "yarn build && electron-builder --macos",
    "dist:win": "yarn build && electron-builder --win",
    "dockerdist:linux": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:linux\"",
    "dockerdist:win": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:win\"",
    "xrelease": "electron-builder --publish onTagOrDraft",
    "release": "electron-builder --publish never"
  },
  "build": {
    "productName": "KISIA Workbench",
    "artifactName": "${productName}.${ext}",
    "directories": {
      "buildResources": "dist-resources"
    },
    "fileAssociations": {
      "ext": [
        "ipynb"
      ]
    },
    "files": [
      "!**/*.d.ts",
      "build/**/*"
    ],
    "extraResources": [
      {
        "from": "venv/bin",
        "to": "venv/bin",
        "filter": [
          "**/j*"
        ]
      },
      {
        "from": "venv/lib/python3.7",
        "to": "venv/lib/python3.7",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "venv/lib/python3.6",
        "to": "venv/lib/python3.6",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "venv/share/jupyter",
        "to": "venv/share/jupyter",
        "filter": [
          "**/*"
        ]
      }
    ],
    "linux": {
      "category": "Development",
      "target": [
        "tar.gz"
      ],
      "desktop": {
        "Categories": "Science;Development;"
      }
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "rpm": {
      "depends": [
        "libXScrnSaver"
      ]
    },
    "nsis": {
      "oneClick": "false",
      "artifactName": "${productName}-Setup.${ext}",
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "language": "1033",
      "installerSidebar": "installerSidebar.bmp"
    },
    "dmg": {
      "icon": "dist-resources/icon.icns",
      "background": "dist-resources/background.png",
      "title": "${productName}-Setup",
      "artifactName": "${productName}.${ext}",
      "window": {
        "height": 380,
        "width": 540
      }
    }
  },
  "author": {
    "name": "Korsimoro",
    "email": "eric@korsimoro.com",
    "url": "http://github.com/korsimoro"
  },
  "bugs": {
    "url": "https://github.com/korsimoro/kisia-toolkit/issues"
  },
  "homepage": "https://github.com/korsimoro/kisia-toolkit",
  "repository": "github:korsimoro/kisia-app",
  "license": "unlicense",
