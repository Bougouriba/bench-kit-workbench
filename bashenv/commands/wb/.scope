#!/bin/bash
# Tasks managing the workbench environment
print_help() {
printf "`cat << EOF
${BLUE}kd wb <SUBCOMMAND> [OPTIONS]${NC}


EOF
`\n\n"
kd_print_subcommand_help_summary wb
}

require_active_environment() {
  if [ "workbench" = "$KITWB_ACTIVE_DEVENV" ]; then
    echo "Workbench Build Environment is Active"
  else
    activate_environment_workbench
  fi
}
